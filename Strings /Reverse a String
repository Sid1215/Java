// Optimized Approach 
class Solution {
    public static String reverseString(String s) {
        StringBuilder ans = new StringBuilder();
        for(int i=s.length()-1; i>=0; i--)
            ans.append(s.charAt(i));
        }
      return ans.toString();
    }
}

// Approach - 2
class solution 
{
  public static String reverseString(String s)
  {
    String ans = "";
    for(int i<s.length()-1; i>=0; i--)
    {
      char ch = s.charAt(i);
      ans += ch;
    }
  return ans;
 }
}
